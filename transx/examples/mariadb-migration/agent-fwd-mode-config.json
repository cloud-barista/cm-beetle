{
  "source": {
    "storageType": "filesystem",
    "path": "/backup",
    "filesystem": {
      "accessType": "ssh",
      "ssh": {
        "host": "localhost",
        "port": 2222,
        "username": "root",
        "privateKeyPath": "./ssh_keys/id_rsa"
      }
    },
    "preCmd": "mariadb-dump -uroot -psource_password --ssl=0 --single-transaction --routines --triggers testdb > /backup/testdb_backup.sql"
  },
  "destination": {
    "storageType": "filesystem",
    "path": "/backup",
    "filesystem": {
      "accessType": "ssh",
      "ssh": {
        "host": "HOST_IP_PLACEHOLDER",
        "port": 2223,
        "username": "root",
        "privateKeyPath": "./ssh_keys/id_rsa"
      }
    },
    "postCmd": "mariadb -uroot -ptarget_password --ssl=0 testdb < /backup/testdb_backup.sql"
  },
  "strategy": "auto"
}