{
  "source": {
    "endpoint": "http://s3.REGION.amazonaws.com",
    "dataPath": "YOUR_SOURCE_BUCKET_NAME/"
  },
  "destination": {
    "endpoint": "http://localhost:1024/spider/s3",
    "dataPath": "YOUR_DESTINATION_BUCKET_NAME/"
  },
  "sourceTransferOptions": {
    "method": "object-storage",
    "objectStorageOptions": {
      "client": "minio",
      "accessKeyId": "YOUR_AWS_ACCESS_KEY_ID",
      "secretAccessKey": "YOUR_AWS_SECRET_ACCESS_KEY",
      "useSSL": true,
      "timeout": 300,
      "maxRetries": 3
    }
  },
  "destinationTransferOptions": {
    "method": "object-storage",
    "objectStorageOptions": {
      "client": "spider",
      "accessKeyId": "YOUR_CONNECTION_CONFIG_NAME",
      "expiresIn": 3600,
      "timeout": 300,
      "maxRetries": 3,
      "verifySSL": false
    }
  }
}